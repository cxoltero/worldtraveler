


<div class="transitions_enabled" id="trips">
  <% @trips.each do |trip|   %>
    <div class="box panel panel-default">
      <%= link_to (image_tag trip.image.url(:medium)), trip %>
      <div class="panel-body">
        <h2>
          <%= link_to trip.title, trip %>
          <p class="user">
          Submited By

          </p>
        </h2>
      </div>
    </div>
  <% end %>
</div>
